{
	"name": "todos",
	"description": "Todos App",
	"private": true,
	"scripts": {
		"build": "npm run build --workspace=@todos/www",
		"start": "npm run start --workspace=@todos/www",
		"dev": "npm run dev --workspace=@todos/www",
		"cypress:open": "cypress open",
		"cypress:run": "cypress run",
		"lint": "npm run lint:packages",
		"lint:packages": "eslint -c eslint.config.js ./packages",
		"prettify:packages": "prettier -c ./packages",
		"g:prettify": "prettier -c \"$INIT_CWD\"",
		"g:lint": "eslint -c eslint.config.js \"$INIT_CWD\""
	},
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"devDependencies": {
		"@eslint/compat": "^1.1.1",
		"@eslint/js": "^9.10.0",
		"@types/node": "^22.5.5",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"cypress": "^13.15.0",
		"eslint": "^9.10.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-node": "^0.3.9",
		"eslint-plugin-import": "^2.30.0",
		"eslint-plugin-jest": "^28.8.3",
		"eslint-plugin-jsx-a11y": "^6.10.0",
		"eslint-plugin-prettier": "^5.2.1",
		"prettier": "^3.3.3",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"ts-node": "^10.9.2",
		"tslib": "^2.7.0",
		"typescript": "^5.5.4"
	},
	"packageManager": "yarn@3.6.3+sha512.d432ab8b22b89cb8b46649ebaf196159b0c1870904224771d75fdf8b55490aaa5de4d9f028deb0425f1513bff7843e97b9d117103795a23b12d2e7c1cedcf50f"
}
